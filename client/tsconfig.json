{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es5",
    "rootDir": ".",
    "jsx": "react-jsx",
    "module": "esnext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "baseUrl": ".",
    "noEmit": true
  },
  "include": [
    "src"
  ],
  "paths": {
    "*": [
      "node_modules/*"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
